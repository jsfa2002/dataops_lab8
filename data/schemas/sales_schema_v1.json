{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "sales",
  "type": "object",
  "properties": {
    "order_id": {
      "type": "string"
    },
    "product_id": {
      "type": "string"
    },
    "quantity": {
      "type": "integer",
      "minimum": 0
    },
    "price": {
      "type": "number",
      "minimum": 0
    },
    "region": {
      "type": "string"
    },
    "order_date": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": [
    "order_id",
    "product_id",
    "quantity",
    "price",
    "order_date"
  ]
}